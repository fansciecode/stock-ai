{"ast":null,"code":"import{createSlice}from'@reduxjs/toolkit';const initialState={users:[],total:0,loading:false,error:null};const usersSlice=createSlice({name:'users',initialState,reducers:{fetchUsersStart:state=>{state.loading=true;state.error=null;},fetchUsersSuccess:(state,action)=>{state.loading=false;state.users=action.payload.users;state.total=action.payload.total;},fetchUsersFailure:(state,action)=>{state.loading=false;state.error=action.payload;},updateUserSuccess:(state,action)=>{const index=state.users.findIndex(user=>user._id===action.payload._id);if(index!==-1){state.users[index]=action.payload;}},deleteUserSuccess:(state,action)=>{state.users=state.users.filter(user=>user._id!==action.payload);state.total-=1;}}});export const{fetchUsersStart,fetchUsersSuccess,fetchUsersFailure,updateUserSuccess,deleteUserSuccess}=usersSlice.actions;export default usersSlice.reducer;","map":{"version":3,"names":["createSlice","initialState","users","total","loading","error","usersSlice","name","reducers","fetchUsersStart","state","fetchUsersSuccess","action","payload","fetchUsersFailure","updateUserSuccess","index","findIndex","user","_id","deleteUserSuccess","filter","actions","reducer"],"sources":["/Users/kirannaik/Desktop/zed/Android-fullstack-ibcm/admin-panel/src/slices/usersSlice.js"],"sourcesContent":["import { createSlice } from '@reduxjs/toolkit';\n\nconst initialState = {\n  users: [],\n  total: 0,\n  loading: false,\n  error: null,\n};\n\nconst usersSlice = createSlice({\n  name: 'users',\n  initialState,\n  reducers: {\n    fetchUsersStart: (state) => {\n      state.loading = true;\n      state.error = null;\n    },\n    fetchUsersSuccess: (state, action) => {\n      state.loading = false;\n      state.users = action.payload.users;\n      state.total = action.payload.total;\n    },\n    fetchUsersFailure: (state, action) => {\n      state.loading = false;\n      state.error = action.payload;\n    },\n    updateUserSuccess: (state, action) => {\n      const index = state.users.findIndex(user => user._id === action.payload._id);\n      if (index !== -1) {\n        state.users[index] = action.payload;\n      }\n    },\n    deleteUserSuccess: (state, action) => {\n      state.users = state.users.filter(user => user._id !== action.payload);\n      state.total -= 1;\n    },\n  },\n});\n\nexport const {\n  fetchUsersStart,\n  fetchUsersSuccess,\n  fetchUsersFailure,\n  updateUserSuccess,\n  deleteUserSuccess,\n} = usersSlice.actions;\n\nexport default usersSlice.reducer; "],"mappings":"AAAA,OAASA,WAAW,KAAQ,kBAAkB,CAE9C,KAAM,CAAAC,YAAY,CAAG,CACnBC,KAAK,CAAE,EAAE,CACTC,KAAK,CAAE,CAAC,CACRC,OAAO,CAAE,KAAK,CACdC,KAAK,CAAE,IACT,CAAC,CAED,KAAM,CAAAC,UAAU,CAAGN,WAAW,CAAC,CAC7BO,IAAI,CAAE,OAAO,CACbN,YAAY,CACZO,QAAQ,CAAE,CACRC,eAAe,CAAGC,KAAK,EAAK,CAC1BA,KAAK,CAACN,OAAO,CAAG,IAAI,CACpBM,KAAK,CAACL,KAAK,CAAG,IAAI,CACpB,CAAC,CACDM,iBAAiB,CAAEA,CAACD,KAAK,CAAEE,MAAM,GAAK,CACpCF,KAAK,CAACN,OAAO,CAAG,KAAK,CACrBM,KAAK,CAACR,KAAK,CAAGU,MAAM,CAACC,OAAO,CAACX,KAAK,CAClCQ,KAAK,CAACP,KAAK,CAAGS,MAAM,CAACC,OAAO,CAACV,KAAK,CACpC,CAAC,CACDW,iBAAiB,CAAEA,CAACJ,KAAK,CAAEE,MAAM,GAAK,CACpCF,KAAK,CAACN,OAAO,CAAG,KAAK,CACrBM,KAAK,CAACL,KAAK,CAAGO,MAAM,CAACC,OAAO,CAC9B,CAAC,CACDE,iBAAiB,CAAEA,CAACL,KAAK,CAAEE,MAAM,GAAK,CACpC,KAAM,CAAAI,KAAK,CAAGN,KAAK,CAACR,KAAK,CAACe,SAAS,CAACC,IAAI,EAAIA,IAAI,CAACC,GAAG,GAAKP,MAAM,CAACC,OAAO,CAACM,GAAG,CAAC,CAC5E,GAAIH,KAAK,GAAK,CAAC,CAAC,CAAE,CAChBN,KAAK,CAACR,KAAK,CAACc,KAAK,CAAC,CAAGJ,MAAM,CAACC,OAAO,CACrC,CACF,CAAC,CACDO,iBAAiB,CAAEA,CAACV,KAAK,CAAEE,MAAM,GAAK,CACpCF,KAAK,CAACR,KAAK,CAAGQ,KAAK,CAACR,KAAK,CAACmB,MAAM,CAACH,IAAI,EAAIA,IAAI,CAACC,GAAG,GAAKP,MAAM,CAACC,OAAO,CAAC,CACrEH,KAAK,CAACP,KAAK,EAAI,CAAC,CAClB,CACF,CACF,CAAC,CAAC,CAEF,MAAO,MAAM,CACXM,eAAe,CACfE,iBAAiB,CACjBG,iBAAiB,CACjBC,iBAAiB,CACjBK,iBACF,CAAC,CAAGd,UAAU,CAACgB,OAAO,CAEtB,cAAe,CAAAhB,UAAU,CAACiB,OAAO","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}